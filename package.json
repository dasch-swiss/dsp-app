{
  "name": "knora-app",
  "version": "2.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/dasch-swiss/knora-app.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "ng build --prod=true --buildOptimizer=true --progress=true",
    "test-ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test-e2e-protractor": "ng e2e --prod=true --protractor-config=./e2e/protractor-ci.conf.js --webdriver-update=false",
    "webdriver-update": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37",
    "yalc-add-modules": "rm -rf .yalc/@knora && yalc add @knora/api && yalc add @knora/action && yalc add @knora/core && yalc add @knora/search && yalc add @knora/viewer && yarn install --prod=false"
  },
  "private": true,
  "dependencies": {
