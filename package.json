    "@angular/forms": "~9.1.9",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@knora/action": "10.0.1",
    "@knora/api": "^0.2.0",
    "@knora/core": "10.0.1",
